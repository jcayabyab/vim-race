{
  "name": "vim-racer",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.30",
    "@fortawesome/free-regular-svg-icons": "^5.14.0",
    "@fortawesome/free-solid-svg-icons": "^5.14.0",
    "@fortawesome/react-fontawesome": "^0.1.11",
    "@testing-library/user-event": "^7.2.1",
    "axios": "^0.21.1",
    "diff": "^4.0.2",
    "lodash": "^4.17.19",
    "moment": "^2.27.0",
    "pg": "^8.3.0",
    "react": "^16.13.1",
    "react-compound-timer": "^1.2.0",
    "react-dom": "^16.13.1",
    "react-modal": "^3.11.2",
    "react-perfect-scrollbar": "^1.5.8",
    "react-redux": "^7.2.1",
    "react-router-dom": "^5.2.0",
    "react-scripts": "3.3.1",
    "react-vim-wasm": "^0.1.4",
    "redux": "^4.0.5",
    "redux-thunk": "^2.3.0",
    "socket.io-client": "^2.3.0",
    "styled-components": "^5.1.1"
  },
  "scripts": {
    "start": "node server/index.js",
    "client": "react-scripts start",
    "server": "nodemon server/index.js",
    "dev": "concurrently \"react-scripts start\" \"npm run server\"",
    "build-sw": "node ./src/sw-build.js",
    "clean-cra-sw": "rm -f build/precache-manifest.*.js && rm -f build/service-worker.js",
    "build": "react-scripts build && npm run build-sw && npm run clean-cra-sw",
    "heroku-postbuild": "npm run install && npm run build && npm install --prefix server",
    "test": "react-scripts test --transformIgnorePatterns \"node_modules/(!?ui-core)/\" --env=jsdom",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.5.0",
    "concurrently": "^5.2.0",
    "http-proxy-middleware": "^1.0.5",
    "nodemon": "^2.0.4",
    "redux-freeze": "^0.1.7",
    "redux-logger": "^3.0.6"
  }
}
